# Criar o arquivo .gitignore
cat > .gitignore << 'EOL'
# Python
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
build/
develop-eggs/
dist/
downloads/
eggs/
.eggs/
lib/
lib64/
parts/
sdist/
var/
wheels/
*.egg-info/
.installed.cfg
*.egg

# Virtual Environment
venv/
ENV/
env/

# IDE
.idea/
.vscode/
*.swp
*.swo

# Game data
data/*.json
!data/cards.json

# Assets
assets/images/*
!assets/images/.gitkeep
assets/sounds/*
!assets/sounds/.gitkeep

# Logs
*.log

# OS
.DS_Store
Thumbs.db
